<%- include("Navbar.ejs") %>

<form action="/admin/save_addmission_detail" method="post">
  <div class="container">
    <div class="row">
      <div class="col-md-12 bg-secondary shadow">
        <h1 class="text-white">MANAGE ADDMISSION</h1>
      </div>
      <div class="col-md-4 mt-5">
        <label for=""> Add Faculty Name</label>
        <input type="text" name="faculty_name" class="form-control" />
      </div>
      <div class="col-md-4 mt-5">
        <label for=""> Add Faculty Designation</label>
        <input type="text" name="faculty_Designation" class="form-control" />
      </div>
      <div class="col-md-4 mt-5">
        <label for=""> Add Faculty photo</label>
        <input type="file" name="faculty_photo" accept="image/*" />
      </div>
      <div class="col-md-2 mt-4">
        <button class="btn btn-primary">Save</button>
      </div>
    </div>
  </div>
</form>

<hr />

